        private void button23_Click(object sender, EventArgs e)
        {
            player.SoundLocation = "pop.wav";
            player.Play();
            a = int.Parse(textBox2.Text);
            c = 30;
            b = a + c;
            textBox2.Text = Convert.ToString(b);
        }

